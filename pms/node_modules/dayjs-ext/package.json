    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "benchmark": "^2.1.4",
    "cross-env": "^5.2.0",
    "date-fns": "^1.29.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.0.0",
    "gzip-size-cli": "^3.0.0",
    "jasmine-core": "^3.3.0",
    "jest": "^23.6.0",
    "karma": "^3.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-sauce-launcher": "^1.2.0",
    "mockdate": "^2.0.2",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.23",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
